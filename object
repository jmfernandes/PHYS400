class Point(object):	"""represents a point in 2-D space"""
print Point
blank = Point()print blank
blank.x = 3.0
blank.y = 4.0
print blank.x
print '(%g, %g)' % (blank.x, blank.y)
import mathdistance = math.sqrt(blank.x**2 + blank.y**2)print distance
def print_point(p):	print '(%g, %g)' % (p.x, p.y)
print_point(blank)

