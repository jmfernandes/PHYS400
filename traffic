a = [0] * 10
V = 1
del a[0]
a.insert(0,1)
print a
for i in range(6):
	t = a.index(1)
	k = 3
	j = t + k*V
	#print j
	if j < len(a):
		if a[t + k*V] == 0:
			#print t
			del a[t]
			a.insert(t,0)
			del a[t + k*V]
			a.insert(t + k*V,1)
	elif j > len(a):
		del a[t]
		a.insert((j-1)-(len(a)),1)
	elif j == len(a):
		del a[t]
		a.insert(0,1)
	print a